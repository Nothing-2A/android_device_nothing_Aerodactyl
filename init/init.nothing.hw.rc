on early-init
    setprop ro.boot.product.hardware.sku ${ro.boot.hardware.sku}

on property:ro.boot.hardware.sku=JPN
    setprop persist.vendor.nfc.config_file_name libnfc-hal-st-st54j.conf

    start gto_secure_element_hal_service_1_2

service gto_secure_element_hal_service_1_2 /vendor/bin/hw/android.hardware.secure_element@1.2-service-gto
    override
    class hal
    user secure_element
    group secure_element nfc
    disabled

service vendor.noth.camera-default /vendor/bin/hw/vendor.noth.hardware.camera-service
    override
    interface aidl vendor.noth.hardware.camera.INtCamService/default
    class hal
    user cameraserver
    group audio camera input drmrpc
    ioprio rt 4
    priority -10
    task_profiles ServiceCapacityRestricted
    disabled

on property:init.svc.camerahalserver=running
    start vendor.noth.camera-default
